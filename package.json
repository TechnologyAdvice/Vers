{
  "name": "vers",
  "version": "0.1.0",
  "description": "Effortless data model versioning for Javascript and Node.js",
  "main": "lib/Vers.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run lint && npm run test-cov && npm run check-cov && npm run karma",
    "lint": "eslint .",
    "karma": "karma start",
    "mocha": "mocha -R spec test",
    "test-cov": "istanbul cover _mocha -- -R spec test",
    "check-cov": "istanbul check-coverage --statements 100 --functions 100 --branches 100 --lines 100",
    "build": "browserify lib/Vers.js -s Vers | derequire | uglifyjs > dist/vers.min.js",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/TomFrost/Vers"
  },
  "keywords": [
    "data",
    "model",
    "version",
    "upgrade",
    "downgrade"
  ],
  "author": "Tom Shawver <tom@frosteddesign.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TomFrost/Vers/issues"
  },
  "homepage": "https://github.com/TomFrost/Vers",
  "devDependencies": {
    "browserify": "^10.2.4",
    "chai": "^2.3.0",
    "chai-as-promised": "^5.0.0",
    "derequire": "^2.0.0",
    "eslint": "^0.21.2",
    "istanbul": "^0.3.14",
    "karma": "^0.12.36",
    "karma-browserify": "^4.2.1",
    "karma-firefox-launcher": "^0.1.6",
    "karma-mocha": "^0.1.10",
    "mocha": "^2.2.5",
    "uglify-js": "^2.4.23"
  },
  "types": "./index.d.ts"
}
